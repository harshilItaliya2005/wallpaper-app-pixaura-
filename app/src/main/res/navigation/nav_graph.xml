<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.pixaura.ui.view.HomeScreen.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_homeFragment_to_wallpaperViewFragment"
            app:destination="@id/wallpaperViewFragment" />
        <action
            android:id="@+id/action_homeFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/categoryFragment"
        android:name="com.example.pixaura.ui.view.categoryScreen.CategoryFragment"
        android:label="fragment_category"
        tools:layout="@layout/fragment_category" >
        <action
            android:id="@+id/action_categoryFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_categoryFragment_to_categoryWallPaperFragment"
            app:destination="@id/categoryWallPaperFragment" />
        <action
            android:id="@+id/action_categoryFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.pixaura.ui.view.searchScreen.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_wallpaperViewFragment"
            app:destination="@id/wallpaperViewFragment" />
        <action
            android:id="@+id/action_searchFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.example.pixaura.ui.view.accountScreen.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" >
        <action
            android:id="@+id/action_accountFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_accountFragment_to_downloadFragment"
            app:destination="@id/downloadFragment" />
        <action
            android:id="@+id/action_accountFragment_to_myAccountFragment2"
            app:destination="@id/myAccountFragment" />
        <action
            android:id="@+id/action_accountFragment_to_galleryFragment"
            app:destination="@id/galleryFragment" />
        <action
            android:id="@+id/action_accountFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/categoryWallPaperFragment"
        android:name="com.example.pixaura.ui.view.fragments.subScreen.CategoryWallPaperFragment"
        android:label="CategoryWallPaperFragment"
        tools:layout="@layout/fragment_category_wallpaper">
        <argument
            android:name="query"
            app:argType="string"
            app:nullable="false" />
        <action
            android:id="@+id/action_categoryWallPaperFragment_to_wallpaperViewFragment"
            app:destination="@id/wallpaperViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/wallpaperViewFragment"
        android:name="com.example.pixaura.ui.view.fragments.wallpaperView.WallpaperViewFragment"
        android:label="fragment_wallpaper_view"
        tools:layout="@layout/fragment_wallpaper_view">

        <argument
            android:name="id"
            app:argType="string"
            app:nullable="false" />

        <argument
            android:name="localUri"
            app:argType="string"
            android:defaultValue=""
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/myAccountFragment"
        android:name="com.example.pixaura.ui.view.fragments.myAccountScreen.MyAccountFragment"
        android:label="fragment_my_account"
        tools:layout="@layout/fragment_my_account" />
    <fragment
        android:id="@+id/downloadFragment"
        android:name="com.example.pixaura.ui.view.fragments.downloadScreen.DownloadFragment"
        android:label="fragment_download"
        tools:layout="@layout/fragment_download" />
    <fragment
        android:id="@+id/galleryFragment"
        android:name="com.example.pixaura.ui.view.fragments.galleryScreen.GalleryFragment"
        android:label="fragment_gallery"
        tools:layout="@layout/fragment_use_own_wallpaper" >
        <action
            android:id="@+id/action_galleryFragment_to_wallpaperViewFragment"
            app:destination="@id/wallpaperViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.pixaura.ui.view.fragments.settingScreen.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/action_settingFragment_to_myAccountFragment"
            app:destination="@id/myAccountFragment" />
    </fragment>


</navigation>